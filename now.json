{
  "version": "2",
  "name": "pr_label_enforcer",
  "alias": "Pull Request Label Enforcer",
  "scale": {
    "all": {
      "min": 1,
      "max": 1
    }
  },
  "builds": [
    {
      "src": "./lib/index.js",
      "use": "@now/node",
      "config": {
        "includeFiles": [
          "node_modules/**"
        ]
      }
    }
  ],
  "env": {
    "APP_ID": "32985",
    "APP_NAME": "WIP",
    "DISABLE_STATS": "true",
    "LOG_LEVEL": "info",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@private_key"
  }
}
